        <IRI>#Trusted_certainty_degree</IRI>
        <Literal xml:lang="en" datatypeIRI="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral">Trusted Certainty Degree</Literal>
    </AnnotationAssertion>
