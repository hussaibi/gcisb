    <AnnotationAssertion>
        <AnnotationProperty IRI="http://purl.org/dc/terms/description"/>
        <IRI>#Resource</IRI>
        <Literal xml:lang="en" datatypeIRI="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral">One or more resources may be allocated to a role for disposition under its authority.</Literal>
